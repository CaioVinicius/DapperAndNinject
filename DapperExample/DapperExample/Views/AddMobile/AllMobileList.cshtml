@model IEnumerable<DapperExample.Models.TBMobileDetails>

@{
    ViewBag.Title = "AllMobileList";
}

<h2>AllMobileList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MobileID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileMeno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileManufactured)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mobileprice)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MobileID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileMeno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileManufactured)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mobileprice)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { MobileID = item.MobileID }) |
            @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |*@
            @Html.ActionLink("Delete", "Delete", new { MobileID = item.MobileID })
        </td>
    </tr>
}

</table>
